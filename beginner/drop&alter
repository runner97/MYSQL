create database student;
show databases;
use student;
create table details(name varchar(50),age int,roll_no varchar(10),years bigint,email varchar(25),mobile_no bigint,cgpa bigint);
describe details;
insert details values("selva",22,"15bmc028",2018,"selva@gmail.com",9874555678,9);
create database employee;
show databases;
use employee;
create table details(employee_name varchar(50),age int,employee_no bigint,years bigint ,mobile_no bigint);
describe details;
insert details values("selva",22,123556,2018,9874555678);
create database production;
show databases;
use production;
create table details(company_name varchar(50),rise_no bigint,years bigint,brand_name varchar(25),mobile_no bigint,model_no bigint);
describe details;
insert details values("selva",22.33,2018,"aaa",9874555678,10);
drop table years;
ALTER TABLE student ADD CARD_NO 32;
